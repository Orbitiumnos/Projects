def kag(n, num):
    k = n
    while k**2 > n:
        k = k-1
    if k == 0:
        return 0
    num = num + 1
    
    if num > 4:
        print('lal',n,k,num)
        #kag(n-k**2, num)
        return num

    num = kag(n-k**2, num)
    print(num)
    
    if num < 5:
        print('num < 5',n, k, num)
    else:
        print('num < 5',n-(k-1)**2, num-1)
        num = kag(n-(k-1)**2, num-1)

n = int(input())
num = 0
kag(n, num)
